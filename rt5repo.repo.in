[rt4repo]
name=RT4 $releasever - $basearch
baseurl=file://@@@REPOBASEDIR@@@/rt4repo/$releasever/$basearch
enabled=0
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1

[rt4repo-source]
name=RT4 $releasever - Source
baseurl=file://@@@REPOBASEDIR@@@/rt4repo/$releasever/SRPMS
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1
