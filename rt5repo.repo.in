[rt5repo]
name=RT5 $releasever - $basearch
baseurl=@@@REPOBASE@@@/rt5repo/el/$releasever/$basearch
enabled=0
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1

[rt5repo-source]
name=RT5 $releasever - Source
baseurl=@@@REPOBASE@@@/rt5repo/el/$releasever/SRPMS
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1
